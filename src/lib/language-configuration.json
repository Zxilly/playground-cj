{
  "comments": {
    "lineComment": "//",
    "blockComment": [
      "/*",
      "*/"
    ]
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "b\"\"\"",
      "close": ""
    },
    {
      "open": "b\"\"",
      "close": ""
    },
    {
      "open": "b\"",
      "close": "\""
    },
    {
      "open": "b''",
      "close": ""
    },
    {
      "open": "b'",
      "close": "'"
    },
    {
      "open": "/*",
      "close": "*/"
    },
    {
      "open": "\"\"\"",
      "close": "\n\"\"\""
    },
    {
      "open": "\"\"",
      "close": ""
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\""
    },
    {
      "open": "'",
      "close": "'"
    },
    {
      "open": "r'",
      "close": "'"
    },
    {
      "open": "r\"",
      "close": "\""
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ],
    [
      "'",
      "'"
    ]
  ],
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}
